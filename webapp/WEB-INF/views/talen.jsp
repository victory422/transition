<script>
//////////// 각 페이지에서 변수 셋팅하는 함수 ////////////
function pagingDataChange(nowPage1, countPerPage1, totalCount1, showElement1, reqUrl1) {
	if (nowPage1 != null) {
		nowPage 		=  nowPage1; 	 
	}
	if (countPerPage1 != null) {
		countPerPage 	=  countPerPage1;
	}
	if (totalCount1 != null) {
		totalCount		=  totalCount1;
	}
	if (showElement1 != null) {
		showElement 	=  showElement1;
	}
	if (reqUrl1 != null) { 
		reqUrl 			=  reqUrl1;
	} 		
}

var nowPage = Number(1); // 현재 페이지 넘버. 최초 진입이므로 1페이지
var countPerPage = Number('12'); // 한 페이지당 리스팅할 갯수
var totalCount = Number('106'); // 전체 data 갯수
var showElement = null; // 가져온 데이터를 append할 엘리먼트
var reqUrl = null; // 호출할 request url
var canCall = true;
var ajaxData = ''; // ajax로 받아온 데이터를 누적시킴
var scrollchk = true; //스크롤 체크 여부

// 더보기 버튼 필요여부 체크
if (totalCount <= (nowPage * countPerPage)) {
	$('#moreview').remove();
} else {
	$('#moreview').show();
}

var firstLoading = 0; // 최초 로딩 시 스크롤 이벤트 제외

$(window).scroll(function() {
	if(scrollchk) {
		//if ($(window).scrollTop() == $(document).height() - $(window).height()) { // 모바일에서 스크롤 감지 안됨
		if (totalCount <= (nowPage * countPerPage)) {
			$('#moreview').remove();
		} else {
			$('#moreview').show();
		}
	
		if (firstLoading == 0) {
			firstLoading = 1;
		} else {
		    if ( $(window).scrollTop() + $(window).height() + 300 > $(document).height() ) {
		        if ( $('#moreview').length != 0 && totalCount > (nowPage * countPerPage) ) {
		        	runmoreview();
		        } else {
			      	// 가져올 것이 없음.
			      	return false;
		        }
		    } 
		}
	}
});

$('body').on("click", "#moreview", function() {
	runmoreview();
});

function runmoreview() {
	if (canCall) {
    	canCall = false;
      	// 다음 페이지 호출 ajax call
      	$.ajax({
	        url:reqUrl,
	        type:'get',
	        data:'&nowPage=' + nowPage,
	        success:function(data){
		        $('#moreview').remove();
	        	// 받아온 데이터 append
		      	$(showElement).append(data);
		      	ajaxData += data; // ajax로 받아온 데이터를 누적시킴
		      	history.replaceState({list:ajaxData, page:nowPage}, 'Page ' + nowPage, '#ajaxCall');
		      	setCookie('list_init', 'n', 1);
		      	scrollchk = true;
		      	canCall = true;
		      	if (totalCount <= (nowPage * countPerPage)) {
		      		$('#moreview').remove();
		      	} else {
		      		$(showElement).append('<div id="moreview"><span>더보기 +</span></div>');
		      	}
		      	// 페이징 데이터 갱신 (nowPage값)
	  			//nowPage++;
	        },
	        error:function(request,status,error){
	        	canCall = true;
	        }
	    })
    }
}
</script>

